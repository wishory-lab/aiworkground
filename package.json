{
  "name": "aiworkground-fullstack",
  "version": "1.0.0",
  "description": "AIWorkground - Your AI Playground for Real Work",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "backend": "cd backend && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:all": "concurrently \"npm run dev\" \"npm run backend\"",
    "setup": "npm install && cd backend && pip install -r requirements.txt",
    "mcp:start": "powershell -ExecutionPolicy Bypass -File ./scripts/run_mcp_servers.ps1 start",
    "mcp:stop": "powershell -ExecutionPolicy Bypass -File ./scripts/run_mcp_servers.ps1 stop",
    "mcp:status": "powershell -ExecutionPolicy Bypass -File ./scripts/run_mcp_servers.ps1 status"
  },
